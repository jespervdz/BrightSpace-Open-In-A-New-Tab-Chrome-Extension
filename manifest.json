{
    "name": "BrightSpace Open Content In A New Tab",
    "version": "0.1",
    "description": "BrightSpace Open Content In A New Tab",
    "manifest_version": 3,
    "author": "Jesper van der Zwaag",
    "web_accessible_resources": [
        {
            "resources": ["openInNewTabD2LButton.html"],
            "matches": [ "<all_urls>" ]
        }
    ],
    "content_scripts": [
        {
            "matches": [ "<all_urls>" ],
            "run_at": "document_idle",
            "all_frames": true,
            "js": ["script.js"]
        }
    ]
}