{
    "name": "BrightSpace PDF Viewer",
    "version": "1.0.0",
    "description": "BrightSpace PDF Viewer",
    "manifest_version": 3,
    "author": "Jesper van der Zwaag",
    "web_accessible_resources": [
        {
            "resources": ["openInNewTabD2LButton.html"],
            "matches": [ "https://brightspace.rug.nl/*", "https://*.brightspace.rug.nl/*", "https://*.brightspace.com/*" ]
        }
    ],
    "content_scripts": [
        {
            "matches": [ "https://brightspace.rug.nl/*", "https://*.brightspace.rug.nl/*", "https://*.brightspace.com/*" ],
            "run_at": "document_end",
            "all_frames": true,
            "js": ["script.js"]
        }
    ]
}